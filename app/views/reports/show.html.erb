
<p id="notice"><%= notice %></p>
<div class="box-blue-c-frame">
<p>
  <b>Title:</b>
  <%= @report.title %>
</p>

<p>
  <b>Description:</b>
  <%= @report.description %>
</p>


  <% if @report.report_photos.size > 0 %>
      <ul class="show_report">
  <% @report.report_photos.order("report_photos.order").each do |photo| %>
    <li><%= image_tag photo.image.url(:middle)%></li>
  <% end %>
      </ul>
  <% end %>



<%#= link_to 'Edit', edit_report_path(@report) %> |
<%#= link_to 'Back', reports_path %>
</div>